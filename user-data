#cloud-config
system_info:
  default_user:
    name: test

chpasswd:
  list: |
    test:weakpassword
  expire: False

chpasswd:
  list: |
    root:weakpassword
  expire: False

ssh_pwauth: True

bootcmd:
  - [ sh, -c, echo "==========bootcmd==========" ]

runcmd:
  - [ sh, -c, echo "==========runcmd==========" ]

final_message: "SYSTEM READY TO LOG IN"

